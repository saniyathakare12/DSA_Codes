#include <iostream>
using namespace std;

#define SIZE 5  // Maximum size of stack

class Stack {
    int items[SIZE];
    int top;

public:
    Stack() {
        top = -1;
    }

    // Push operation
    void push(int element) {
        if (top == SIZE - 1) {
            cout << "Stack Overflow! Cannot push " << element << endl;
        } else {
            items[++top] = element;
            cout << element << " pushed to stack." << endl;
        }
    }

    // Pop operation
    void pop() {
        if (top == -1) {
            cout << "Stack Underflow! Stack is empty." << endl;
        } else {
            cout << items[top--] << " popped from stack." << endl;
        }
    }

    // Display stack elements
    void display() {
        if (top == -1) {
            cout << "Stack is empty." << endl;
        } else {
            cout << "Stack elements: ";
            for (int i = top; i >= 0; i--)
                cout << items[i] << " ";
            cout << endl;
        }
    }
};

int main() {
    Stack s;

    s.push(10);
    s.push(20);
    s.push(30);
    s.display();

    s.pop();
    s.display();

    return 0;
}
